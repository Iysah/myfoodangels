{
  "expo": {
    "name": "Confluenxe",
    "slug": "confluenx-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000080"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theproductdude.confluenxapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#000080"
      },
      "package": "com.theproductdude.confluenxapp",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-screen.png",
          "resizeMode": "contain",
          "backgroundColor": "#000080"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them in chat.",
          "cameraPermission": "The app accesses your camera to let you take photos for chat."
        }
      ],
      "expo-document-picker"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b0879115-f6a8-49dc-b601-65224fd9b149"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b0879115-f6a8-49dc-b601-65224fd9b149"
    }
  }
}
