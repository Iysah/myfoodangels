{
  "expo": {
    "name": "MyFoodAngels",
    "slug": "myfoodangels",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mfa-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/mfa-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theproductdude.myfoodangels"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mfa-icon.png",
        "backgroundColor": "#000080"
      },
      "package": "com.theproductdude.myfoodangels",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them in chat.",
          "cameraPermission": "The app accesses your camera to let you take photos for chat."
        }
      ],
      "expo-document-picker"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b0879115-f6a8-49dc-b601-65224fd9b149"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b0879115-f6a8-49dc-b601-65224fd9b149"
    }
  }
}
